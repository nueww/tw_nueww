                            </label>
                            <config>
                                <type>inline</type>
                                <minitems>0</minitems>
                                <maxitems>99</maxitems>
                                <appearance type="array">
                                    <createNewRelationLinkTitle>LLL:EXT:cms/locallang_ttc.xlf:media.addFileReference
                                    </createNewRelationLinkTitle>
                                    <enabledControls type="array">
                                        <delete>1</delete>
                                        <dragdrop>1</dragdrop>
                                        <hide>1</hide>
                                        <info>1</info>
                                        <localize>1</localize>
                                    </enabledControls>
